        spawn("Renegade_Bow", 0, 1);
        spawn("Renegade_Spear", 0, 1);
        spawn("Renegade_Grenadier", 0, 1);
        probability(1.0);
        category_cooldown(10.0);
        first_spawn_delay(3.0);
        spawn_delay(0.5);
        use_human_spawn_points(true);
    }
