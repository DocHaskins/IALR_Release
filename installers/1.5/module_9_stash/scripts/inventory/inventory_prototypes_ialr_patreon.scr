sub bundle_stuff()
{
		ItemType(ItemType_ItemBundle);
        Visibility(true);
        MaxStackCount(1);
        Mesh("loot_ok_ammo_short.msh");
        HudIcon("weapon_throw_knives");
        Skin("default");
        use PickupSounds_Default();
        Color(Color_White);
        Price(20);
        GameVersion(1);
}

